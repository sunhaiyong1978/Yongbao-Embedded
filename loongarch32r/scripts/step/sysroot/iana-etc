source env/sysroot/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=sysroot
export STEP_PACKAGENAME=iana-etc
export PACKAGE_VERSION=20251016
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/sysroot/iana-etc/20251016/
rm -rf ${BUILD_DIRECTORY}/iana-etc-20251016
tar xvf ${DOWNLOADDIR}/iana-etc-20251016.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/iana-etc-20251016
	cp -v services protocols ${SYSROOT_DIR}/etc
popd 
rm -rf ${BUILD_DIRECTORY}/iana-etc-20251016
