source env/sysroot/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=sysroot
export STEP_PACKAGENAME=man-pages
export PACKAGE_VERSION=6.16
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/sysroot/man-pages/6.16/
rm -rf ${BUILD_DIRECTORY}/man-pages-6.16
tar xvf ${DOWNLOADDIR}/man-pages-6.16.tar.xz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/man-pages-6.16
	make -R prefix=${PREFIX} DESTDIR=${SYSROOT_DIR} install
popd 
rm -rf ${BUILD_DIRECTORY}/man-pages-6.16
